{
  "expo": {
    "name": "Liguster",
    "slug": "liguster-web-expo",
    "scheme": "liguster",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon-1024.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "buildNumber": "4",
      "supportsTablet": true,
      "bundleIdentifier": "dk.liguster.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "Vi bruger din position til at vise dig på kortet og finde relevante ting i nærheden af dig.",
        "NSPhotoLibraryUsageDescription": "Vi skal bruge adgang til dine billeder for at du kan uploade billeder til aktiviteter og opslag.",
        "NSPhotoLibraryAddUsageDescription": "Vi skal bruge adgang til at gemme billeder, når du downloader eller redigerer.",
        "NSCameraUsageDescription": "Vi skal bruge adgang til kameraet, hvis du vil tage et billede direkte i appen."
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },

    "android": {
      "package": "dk.liguster.app",
      "versionCode": 3,
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#131921"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": false,
          "data": [{ "scheme": "liguster" }],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },

    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#131921"
    },

    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-2000.png",
          "resizeMode": "contain",
          "backgroundColor": "#131921"
        }
      ],
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "Vi skal bruge adgang til dine billeder for at uploade billeder til aktiviteter og opslag.",
          "cameraPermission": "Vi skal bruge adgang til kameraet, hvis du vil tage et billede direkte i appen."
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": { "deploymentTarget": "15.1", "newArchEnabled": true }
        }
      ]
    ],

    "experiments": { "typedRoutes": true, "tsconfigPaths": true },

    "assetBundlePatterns": ["**/*"],

    "extra": {
      "router": {},
      "eas": { "projectId": "510eb874-f1b3-4c29-9c58-4d19f0dd6272" },
      "supabaseUrl": "https://gizskyfynvyvhnaqcyax.supabase.co",
